@model FreteFree.Models.Motorista.OrdemCarregamento



<style>


    table.table {
        line-height: 9pt;
        border: "0";
        width: "100%";
    }

    table.limi {
        width: 17cm;
        line-height: 9pt;
    }

    td.limite {
        border: solid windowtext .5pt;
        padding: 5.65pt 5.65pt 5.65pt 5.65pt
    }
</style>

<div align="center">
    <div class="container content-wrapper">
        <div class="conteudo">
            <div class="row">
                <div class="col-md-12 content">
                    <div class="row">
                        @*<div class="col-md-12" align="center">
                                <h2>Ordem de Carregamento e Recibo</h2>
                                <br>
                            </div>*@
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div id="principal">
                                <table id="limi" border="1" cellspacing="0">
                                    <tbody>
                                        <tr>
                                            <td id="limite">
                                                <table id="tabla">
                                                    <!-- <tbody><tr> -->
                                                    <td valign="middle" align="left" width="60" height="60">
                                                        <img width="100" height="80" src="~/download.png" alt="Brasão" border="0">
                                                    </td>
                                                    <div id="folha">
                                                        <td id="folha" align="center">

                                                            <p style="margin:0cm; margin-bottom:0pt;">&nbsp;</p>

                                                            <font face="Arial" size="4">
                                                                <b>
                                                                    ORDEM DE CARREGAMENTO
                                                                </b>
                                                            </font>

                                                            <p style="margin:0cm; margin-bottom:0pt;">&nbsp;</p>

                                                            <!-- <p style="margin:0cm; margin-bottom:0pt;">&nbsp;</p> -->

                                                            <font face="Arial">
                                                                <b>
                                                                    BETÃO
                                                                </b>
                                                            </font>
                                                            <p style="margin:0cm; margin-bottom:0pt;">&nbsp;</p>

                                                            <font face="Arial">
                                                                <b>
                                                                    TEMOS CARGA PARA TODO PAÍS
                                                                </b>
                                                            </font>
                                                            <p style="margin:0cm; margin-bottom:0pt;">&nbsp;</p>

                                                            <font face="Arial" style="font-size: 8pt">
                                                                <b>
                                                                    RUA JOÃO PEDRO MOREIRA DE VARVALHO - Fone: 3531-7600 Cep: 78555-000 SINOP/MT
                                                                </b>
                                                            </font>

                                                            <!-- <p style="margin:0cm; margin-bottom:0pt;">&nbsp;</p> -->

                                                        </td>
                                                    </div>
                                                    <td valign="right" align="center" width="90" height="100">

                                                    <td id="borda" width="20%" valign="left" style="
                                            BORDER-RIGHT: windowtext 0.5pt solid;
                                            BORDER-TOP: windowtext 0.5pt solid;
                                            BORDER-LEFT: windowtext 0.5pt solid;
                                            BORDER-BOTTOM: windowtext 0.5pt solid;
                                            PADDING-LEFT: 1.5pt;
                                            padding-bottom: 3.5pt;
                                            ">

                                                        <center>
                                                            <font face="Arial" style="font-size:12pt">
                                                                Ordem de Carregamento Nº:
                                                            </font>
                                                            <br><br><br>
                                                            <font face="Arial" style="font-size: 12pt">
                                                                <b>  @Html.DisplayFor(model => model.OrdemCarregamentoId)</b>
                                                            </font>
                                                            <br>
                                                    </td>
                                        </tr>
                                        </center>
                                    </tbody>
                                </table>
                                <!-- Fim Cabeçalho do Cartão -->
                                <hr>
                                <!-- Coluna 1 -->
                                <p style="margin:0cm; margin-bottom:0pt;"></p>
                                <table border="0" width="100%">
                                    <tbody>
                                    <td width="60%" valign="top">
                                        <font face="Arial" style="font-size: 12pt">
                                            Solicitamos ao Sr:<b> <u>   @Html.DisplayFor(model => model.Empresa.NomeResponsavel)  </u></b>
                                        </font>
                                        <br>
                                        <font face="Arial" style="font-size: 12pt">
                                            da Empresa:<b><u>   @Html.DisplayFor(model => model.Empresa.NomeEmpresa)  </u></b>
                                        </font>
                                        <br>
                                        <font face="Arial" style="font-size: 12pt">
                                            A gentileza procede, a carregamento do caminhão: <b><u> @Html.DisplayFor(model => model.Motorista.TipoCaminhao)  </u></b>
                                        </font>
                                        <br>
                                        <font face="Arial" style="font-size: 12pt">

                                            Placa Cavalo: <b>
                                                <u> @Html.DisplayFor(model => model.Motorista.PlacaCavalo)</u>
                                                &nbsp; - &nbsp; <u> @Html.DisplayFor(model => model.Motorista.CidadeCavalo)</u>
                                                &nbsp; - &nbsp; <u> @Html.DisplayFor(model => model.Motorista.EstadoCavalo)</u>
                                            </b>
                                        </font>
                                        <br>
                                        <font face="Arial" style="font-size: 12pt">
                                            Proprietário: <b><u> @Html.DisplayFor(model => model.Motorista.CaminhaoProprietario)  </u></b>
                                        </font>
                                        <br>
                                        <font face="Arial" style="font-size: 12pt">
                                            Endereço Proprietário: <b><u> @Html.DisplayFor(model => model.Motorista.EnderecoProprietario)  </u></b>
                                        </font>
                                        <br>
                                        <font face="Arial" style="font-size: 12pt">
                                            Nome Motorista: <b><u> @Html.DisplayFor(model => model.Motorista.NomeMotorista)  </u></b>
                                        </font>
                                        <br>
                                        <font face="Arial" style="font-size: 12pt">
                                            Endereço Motorista: <b><u> @Html.DisplayFor(model => model.Motorista.EnderecoMotorista)  </u></b>
                                        </font>
                                        <br>
                                        <font face="Arial" style="font-size: 12pt">
                                            Fone Motorista: <b><u> @Html.DisplayFor(model => model.Motorista.TelefoneMovel)  </u></b>
                                        </font>

                                        <br>
                                        <font face="Arial" style="font-size: 12pt">
                                            Telefone: <b>
                                                <u> @Html.DisplayFor(model => model.Motorista.TelefoneMovel)</u>
                                                &nbsp; - &nbsp; Telefone Fixo: <u> @Html.DisplayFor(model => model.Motorista.TelefoneFixo)</u>
                                                &nbsp; - &nbsp; Cidade: <u> @Html.DisplayFor(model => model.Motorista.CidadeMotorista)</u>
                                                &nbsp; - &nbsp; UF: <u> @Html.DisplayFor(model => model.Motorista.EstadoMotorista)</u>
                                            </b>
                                        </font>
                                        <br>
                                        <font face="Arial" style="font-size: 12pt">
                                            CPF: <b>
                                                <u> @Html.DisplayFor(model => model.Motorista.TelefoneMovel)</u>
                                                &nbsp; - &nbsp; RG: <u> @Html.DisplayFor(model => model.Motorista.TelefoneFixo)</u>
                                                &nbsp; - &nbsp; CNPJ: <u> @Html.DisplayFor(model => model.Motorista.CidadeMotorista)</u>
                                                &nbsp; - &nbsp; UF: <u> @Html.DisplayFor(model => model.Motorista.EstadoMotorista)</u>
                                            </b>
                                        </font>

                                    </td>
                                    <td width="2%" border="0"> </td>
                                    <!-- Coluna 2 -->
                                    <td width="60%" valign="top">

                                        <font face="Arial" style="font-size: 12pt">
                                            Fone: <b><u>    @Html.DisplayFor(model => model.Empresa.TelefoneEmpresa)  </u> </b>
                                        </font><br>
                                        <font face="Arial" style="font-size: 12pt">
                                            Cidade: <b><u>   @Html.DisplayFor(model => model.Empresa.CidadeEmpresa)  </u></b>
                                        </font>
                                        <br>
                                        <br>
                                        <font face="Arial" style="font-size: 12pt">
                                            
                                                Placa Cavalo:<b><u> @Html.DisplayFor(model => model.Motorista.PlacaCarreta)</u>
                                                &nbsp; - &nbsp; <u> @Html.DisplayFor(model => model.Motorista.CidadeCarreta)</u>
                                                &nbsp; - &nbsp; <u> @Html.DisplayFor(model => model.Motorista.EstadoCarreta)</u>
                                            </b>
                                        </font>
                               

                                    </td>
                                </table>





                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<br>
<br>
<div>

    <a asp-action="Index">Voltar</a>
    <input type="button" onclick="printPage()" value="Print" />

</div>
<br>
